{
    "colors": {
        "primaryColor": "#5E49FF",
        "greenWaterColor": "#25CBD3",
        "greenColor": "#3DB42D",
        "lightBlueColor": "#F0F5FF",
        "whiteColor": "#FFFFFF",
        "grayColor00": "#F4F4F4",
        "grayColor01": "#DADADA",
        "grayColor02": "#7C7786",
        "grayColor03": "#4D4DCD4",
        "grayCOlor04": "#333333"
    },
  "expo": {
    "name": "devagram",
    "slug": "devagram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "O aplicativo Devagram, deseja ter acesso a galeria do seu dispositivo."
          }
        ]
      ]
  }
}
